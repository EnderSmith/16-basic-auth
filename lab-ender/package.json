{
  "name": "lab-ender",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest -i",
    "test:watch": "jest -i --watchAll",
    "test:debug": "DEBUG=serve* jest -i",
    "test:debug-win": "set DEBUG=serve* & jest -i",
    "start:watch": "nodemon index.js",
    "start:debug": "DEBUG=serve* nodemon index.js",
    "start:debug-win": "set DEBUG=serve* & nodemon index.js",
    "start:debug-win:once": "set DEBUG=serve* & node index.js",
    "start:debug-win:v": "set DEBUG=serve*,verbo* & nodemon index.js",
    "linter": "eslint .",
    "start-db": "mkdir -p ./data/db && mongod --dbpath ./data/db",
    "stop-db": "killall mongod"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/__test__/"
    ],
    "coverageDirectory": "./__test__/coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "author": "endersmith",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.1",
    "mongoose": "^5.0.3"
  },
  "devDependencies": {
    "debug": "^3.1.0",
    "eslint": "^4.16.0",
    "jest": "^22.1.4"
  }
}
